<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">Circle-ImageView</string>

    <declare-styleable name="CircleImageView">
        <attr name="src" format="reference" />

        <attr name="bottomCircleOnColor" format="color" />
        <attr name="bottomCircleOffColor" format="color" />
        <attr name="bottomCircleIndent" format="dimension" />

        <attr name="middleCircleOnColor" format="color" />
        <attr name="middleCircleOffColor" format="color" />
        <attr name="middleCircleIndent" format="dimension" />
    </declare-styleable>

    <string-array name="action_list">
        <item>RoundRect</item>
        <item>Picture</item>
        <item>ClipPath</item>
        <item>Natural</item>
    </string-array>

</resources>
